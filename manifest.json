{
    "manifest_version": 3,
    "name": "Auto Searcher",
    "version": "0.1.3",
    "description": "Search multiple predefined sites at the same time.",
    "author": "Krasen Ivanov",
    "icons": {
        "16": "icons/auto_search-16.png",
        "32": "icons/auto_search-32.png",
        "64": "icons/auto_search-64.png",
        "128": "icons/auto_search-128.png"
    },
    "action": {
        "default_title": "Click to use the searcher",
        "default_popup": "auto_search.html",
        "default_icon": {
            "16": "icons/auto_search-16.png",
            "32": "icons/auto_search-32.png",
            "64": "icons/auto_search-64.png",
            "128": "icons/auto_search-128.png"
        }
    },
    "options_page": "options.html"
}